<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="pack.beans">
	<!-- Paper类映射到paper_inf表 -->
	<class name="Paper" table="paper_inf">
		<id name="id" column="paper_id">
			<generator class="assigned"/>
		</id>
		<property name="typesId" type="int" column="types_id"/>
		<property name="title" type="string" column="paper_title"/>
		<property name="valid" type="boolean" column="paper_valid"/>
		<property name="flag" type="int" column="paper_flag"/>
	</class>
</hibernate-mapping>