<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="pack.beans">
	<class name="MGFactor" table="MGFactor_inf">
		<composite-id>
			<key-property name="userId" column="user_id" type="int"/>
			<key-property name="id" column="MGFactor_id" type="int"/>
		</composite-id>
		<property name="title" column="MGFactor_title" type="string"/>
		<property name="score" column="MGFactor_score" type="int"/>
		<property name="valid" column="MGFactor_valid" type="boolean"/>
		<property name="flag" column="MGFactor_flag" type="int"/>
	</class>
</hibernate-mapping>