<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="pack.beans">
	 <class name="Report" table="report">
		<id name="id" column="report_id">
			<generator class="increment"/>
		</id>
		<property name="date"
			type="java.util.Date" column="report_date"/>
		
		<property name="ordersum"
			type="int" column="report_ordersum"/>
		
		<property name="checkin"
			type="int" column="report_chechin"/>
		
		<property name="income"
			type="int" column="report_income"/>		
		
	</class>
</hibernate-mapping>