<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="pack.beans">
	<class name="PaperValid" table="paperValid_inf">
		<composite-id>
			<key-property name="paperId" type="int" column="paper_id"/>
			<key-property name="userId" type="int" column="user_id"/>
		</composite-id>
		<property name="valid" type="boolean" column="paperValid_valid"/>
		<property name="flag" type="int" column="paperValid_flag"/>
	</class>
</hibernate-mapping>