<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="pack.beans">
	<class name="MatchTypes" table="matchTypes_inf">
		<id name="id" column="matchTypes_id">
			<generator class="assigned"/>
		</id>
		<property name="title" column="matchTypes_title"
			type="string"/>
		<property name="valid" column="matchTypes_valid"
			type="boolean"/>
		<property name="flag" column="matchTypes_flag"
			type="int"/>
	</class>
</hibernate-mapping>