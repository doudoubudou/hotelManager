<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="pack.beans">
	<class name="Education" table="education_inf">
		<id name="id" column="education_id">
			<generator class="identity"/>
		</id>
		<property name="name" type="string" column="education_name"/>
		<property name="valid" type="boolean" column="education_valid"/>
		<property name="flag" type="int" column="education_flag"/>
	</class>
</hibernate-mapping>