<?xml version="1.0" encoding="GBK"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<!-- loginAction -->
	<bean id="loginAction" class="pack.action.LoginAction">
		<property name="getService" ref="getService"/>
		<property name="loginService" ref="loginService"/>
	</bean>
	<!-- bookAction -->
	<bean id="bookAction" class="pack.action.BookAction">
		<property name="saveService" ref="saveService"/>
		<property name="getService" ref="getService"/>
		<property name="sortService" ref="sortService"/>
	</bean>
	<!-- checkInAction -->
	<bean id="checkInAction" class="pack.action.CheckInAction">
		<property name="sortService" ref="sortService"/>
		<property name="getService" ref="getService"/>
		<property name="saveService" ref="saveService"/>
	</bean>
	<!-- checkOutAction -->
	<bean id="checkOutAction" class="pack.action.CheckOutAction">
		<property name="getService" ref="getService"/>
	</bean>
	<!-- customerAction -->
	<bean id="customerAction" class="pack.action.CustomerAction">
		<property name="getService" ref="getService"/>
	</bean>
	<!-- autoCompleteAction -->
	<bean id="autoCompleteAction" class="pack.action.AutoCompleteAction">
		<property name="getService" ref="getService"/>
	</bean>
	<!-- recordsQuery -->
	<bean id="recordsQueryAction" class="pack.action.RecordsQueryAction">
		<property name="getService" ref="getService"/>
	</bean>
	<!-- reportAction -->
	<bean id="reportAction" class="pack.action.ReportAction">
		<property name="getService" ref="getService"/>
		<property name="saveService" ref="saveService"/>
	</bean>
	<!-- bookQueryAction -->
	<bean id="bookQueryAction" class="pack.action.BookQueryAction">
		<property name="getService" ref="getService"/>
	</bean>
	<!-- bookOptionAction -->
	<bean id="bookOptionAction" class="pack.action.BookOptionAction">
		<property name="saveService" ref="saveService"/>
		<property name="getService" ref="getService"/>
	</bean>
	<!-- ioQueryAction -->
	<bean id="ioQueryAction" class="pack.action.IoQueryAction">
		<property name="getService" ref="getService"/>
	</bean>
	<!-- ioOptionAction -->
	<bean id="ioOptionAction" class="pack.action.IoOptionAction">
		<property name="saveService" ref="saveService"/>
		<property name="getService" ref="getService"/>
	</bean>
</beans>