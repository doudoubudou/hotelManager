<?xml version="1.0" encoding="GBK"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<!-- AdminDao -->
	<bean id="adminDao" class="pack.dao.impl.AdminDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"/>
	</bean>
	<!-- RoomDao -->
	<bean id="roomDao" class="pack.dao.impl.RoomDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"/>
	</bean>
	<!-- BookDao -->
	<bean id="bookDao" class="pack.dao.impl.BookDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"/>
	</bean>
	<!-- IoDao -->
	<bean id="ioDao" class="pack.dao.impl.IoDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"/>
	</bean>
	<!-- TypeDao -->
	<bean id="typeDao" class="pack.dao.impl.TypeDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"/>
	</bean>
	<!-- CustomerDao -->
	<bean id="customerDao" class="pack.dao.impl.CustomerDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"/>
	</bean>
	<!-- ReportDao -->
	<bean id="reportDao" class="pack.dao.impl.ReportDaoImpl">
		<property name="sessionFactory" ref="sessionFactory"/>
	</bean>
</beans>